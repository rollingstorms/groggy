{
  "name": "groggy-widgets",
  "version": "0.1.0",
  "description": "Interactive Jupyter widget for Groggy graph visualization",
  "keywords": [
    "jupyter",
    "widget",
    "graph",
    "visualization",
    "interactive"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/groggy-dev/groggy.git"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:standalone": "webpack --config webpack.standalone.config.js",
    "watch": "webpack --watch --mode=development",
    "clean": "rimraf lib dist",
    "prepare": "npm run clean && npm run build:prod"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3 || ^4 || ^5 || ^6",
    "@jupyter-widgets/controls": "^1.1.10 || ^2 || ^3 || ^4 || ^5",
    "@jupyterlab/application": "^3.4.0",
    "@types/node": "^18.0.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3 || ^4 || ^5 || ^6"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "groggy/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true,
        "strictVersion": false
      }
    }
  }
}